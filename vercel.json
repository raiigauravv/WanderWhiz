{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "GOOGLE_MAPS_API_KEY": "@google_maps_api_key",
    "OPENAI_API_KEY": "@openai_api_key", 
    "SECRET_KEY": "@secret_key"
  }
}
